<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ado.edu.itla.taskapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vista.CategoriaActivity" />
        <activity android:name=".vista.CategoriaListaActivity" />
        <activity android:name=".vista.Login" />
        <activity android:name=".vista.Registro" />
        <activity android:name=".vista.EstadoTareaNormal" />
        <activity android:name=".vista.TareaUsuarioNormal" />
        <activity android:name=".vista.EstadoTareas" />
        <activity android:name=".vista.EstadoTareaTecnico" />
        <activity android:name=".vista.AsignarTarea" />
        <activity android:name=".vista.TareaTecnico"></activity>
    </application>

</manifest>